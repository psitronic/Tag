{
  "manifest_version": 2,

  "name": "stickertags",
  "description": "tags people left on websites",
  "version": "0.0.1",

  "browser_action": {
   "default_icon": "icon_32_active.png",
   "default_popup": "popup.html"
  },
  "permissions": [
    "background",
    "tabs",
    "activeTab",
    "storage",
    "https://www.mocky.io/*"
    "browserAction"
   ],
  "background":{
    "scripts": ["event_script.js"],
    "persistent": false
  }
}
